<Window x:Class="memword.Practice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:memword"
        mc:Ignorable="d"
        Title="Practice" Height="450" Width="800">
    <Grid >
        <DockPanel Background="#FFEAD3E7">
            <Rectangle Fill="#FFCBA1C7" DockPanel.Dock="Left" Width="200" HorizontalAlignment="Left"/>
        </DockPanel>
        <StackPanel x:Name="menu_SP" HorizontalAlignment="Left" VerticalAlignment="Stretch" Width="200">
            <TextBlock Text="Мемворд" Margin="0,10,0,0" FontSize="30" TextAlignment="Center" Width="200" Height="35" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Mandy's Sketch"/>
        </StackPanel>
        <StackPanel x:Name="main_SP" HorizontalAlignment="Stretch" Margin="200,0,0,0" VerticalAlignment="Stretch">
            <TextBlock Text="Привет" HorizontalAlignment="Center" Width="651" Height="100"/>
        </StackPanel>
        


    </Grid>
</Window>
